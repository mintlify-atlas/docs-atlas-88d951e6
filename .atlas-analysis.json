  "publicApiSurface": [
    "POST /attach - Attach products to customers",
    "POST /check - Check feature access and balances",
    "POST /track - Record usage events",
    "POST /cancel - Cancel customer subscriptions",
    "POST /checkout - Create checkout sessions",
    "POST /setup_payment - Setup payment methods",
    "POST /customers/{customer_id}/billing_portal - Access billing portal",
    "POST /query - Query usage analytics",
    "GET /customers - List customers",
    "POST /customers - Create customer",
    "GET /customers/{customer_id} - Get customer",
    "PATCH /customers/{customer_id} - Update customer",
    "DELETE /customers/{customer_id} - Delete customer",
    "GET /products - List products/plans",
    "POST /products - Create product",
    "GET /products/{product_id} - Get product",
    "PATCH /products/{product_id} - Update product",
    "DELETE /products/{product_id} - Delete product",
    "GET /features - List features",
    "POST /features - Create feature",
    "GET /entities - List entities",
    "POST /entities - Create entity",
    "GET /events - List usage events",
    "POST /events/aggregate - Aggregate events"
  ]
